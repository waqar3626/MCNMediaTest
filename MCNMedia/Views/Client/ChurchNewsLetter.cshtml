@model MCNMedia_Dev.Models.GenericModel
@{
    ViewData["Title"] = "ChurchNewsLetter";
    Layout = "~/Views/Shared/_NestedLayout.cshtml";
}
<div class="row m-0 mb-3">
    <div class="col-lg-12">
        <div class="alert alert-danger" id="addNewLetterListErrorMessage" hidden>

        </div>

    </div>

</div>
<div class="row m-0 mb-3">
    <div class="col-lg-12">
        <div class="alert alert-success" id="addNewLetterListSuccessMessage" hidden>

        </div>

    </div>

</div>
<div class="row">
    <div class="col-sm-12 grid-margin stretch-card d-none d-md-flex w-100">
        <div class="card">
            <div class="card-body">
                <div class="row">
                    <div class="col-sm-12">
                        @Html.Partial("~/Views/ChurchNewsLetter/_ChurchNewLetter.cshtml")
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
