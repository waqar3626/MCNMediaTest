
@{
    ViewData["Title"] = "SubscriberDetail";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<script type="text/javascript">

    $(document).ready(function () {

        var curTabName = '@ViewBag.TabName';
        if (curTabName == '') {
            $("#Camera-tab").click();
        }
        else {
            $("#"+curTabName+"-tab").click();
        }

});

</script>

<div class="grid-margin stretch-card">
    <div class="card">
        <div class="card-body">
            <h4 class="card-title">Subscriber Detail</h4>
            <h3>@ViewData["message"]</h3>
            <ul class="nav nav-tabs small" role="tablist">
                <li class="nav-item">
                    <a class="nav-link active" id="Camera-tab" data-toggle="tab" href="#Camera-1" role="tab" aria-controls="Camera-1" aria-selected="true">
                    @*<i class="fa fa-camera-retro text-success"></i>*@
                    Profile</a>
                </li>
                
                <li class="nav-item">
                    <a class="nav-link" id="Slideshow-tab" data-toggle="tab" href="#Slideshow-1" role="tab" aria-controls="Slideshow-1" aria-selected="false">
                    @*<i class="mdi mdi-folder-multiple-image text-info"></i>*@
                    Subscription Detail</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" id="Video-tab" data-toggle="tab" href="#Video-1" role="tab" aria-controls="Video-1" aria-selected="false">
                    <i class="fa fa-file-video-o text-danger"></i>
                    </a>
                </li>
            </ul>
            <div class="tab-content">

                
                <div class="tab-pane fade" id="Picture-1" role="tabpanel" aria-labelledby="Picture-tab">
                    <div class="media">
                        @*<div class="media-body">*@
                            @*@Html.Partial("~/Views/MediaChurch/_Pictures.cshtml")*@
                        @*</div>*@
                    </div>
                </div>



               
                <div class="tab-pane fade" id="Video-1" role="tabpanel" aria-labelledby="Video-tab">
                    <div class="media">
                        @*<div class="media-body">*@
                            @*@Html.Partial("~/Views/Subscriber/_SubscriberHistory.cshtml")*@
                        @*</div>*@
                    </div>
                </div>
                
                <div class="tab-pane fade" id="Announcement-1" role="tabpanel" aria-labelledby="Announcement-tab">
                    <div class="media">
                        @*<div class="media-body">*@
                            @*@Html.Partial("~/Views/Announcement/_ListAnnouncement.cshtml")*@
                        @*</div>*@
                    </div>
                </div>

                <div class="tab-pane fade" id="Schedule-1" role="tabpanel" aria-labelledby="Schedule-tab">
                    <div class="media">
                        @*<div class="media-body">*@
                            @*@Html.Partial("~/Views/ChurchSchedule/_ListSchedule.cshtml")*@
                        @*</div>*@
                    </div>
                </div>

                <div class="tab-pane fade" id="NewsLetter-1" role="tabpanel" aria-labelledby="NewsLetter-tab">
                    <div class="media">
                        @*<div class="media-body">*@
                            @*@Html.Partial("~/Views/ChurchNewsLetter/_ListChurchNewsLetter.cshtml")*@
                        @*</div>*@
                    </div>
                </div>
                <div class="tab-pane fade show active" id="Camera-1" role="tabpanel" aria-labelledby="Camera-tab">
                    <div class="media">
                        <div class="media-body">
                            @*@Html.Partial("~/Views/Camera/_Camera.cshtml")*@
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>


